{
    "name": "kenjis/ore-ore-php",
    "description": "A simple and fast PHP 5.4+ glue framework with minimal learning cost.",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/kenjis/Dice"
        }
    ],
    "require": {
        "php": ">=5.4",
        "twig/twig": "1.*",
        "corneltek/pux": "~1.3",
        "pimple/pimple": "dev-master",
        "jasrags/dice": "dev-develop"
    },
    "require-dev": {
        "codeception/aspect-mock": "*",
        "mockery/mockery": "dev-master@dev"
    },
   "autoload": {
      "psr-4": {
          "kenjis\\OreOrePHP\\": "src/",
          "Controller\\": "app/controllers/",
          "Model\\": "app/models/"
      }
  }
}
